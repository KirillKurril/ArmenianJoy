<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Backgammon.UI.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Backgammon.UI"
    Shell.FlyoutBehavior="Disabled"
    xmlns:pages="clr-namespace:Backgammon.UI.Pages"
    Title="Backgammon.UI">

    <ShellContent
        ContentTemplate="{DataTemplate pages:MenuView}"
        Route="Menu" />

</Shell>
